<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.ysu.zyw.tc.components.aliyun.oss.TcAliyunOssService">
        <property name="domainName" value="${properties.aliyun.oss.domain.name}"/>
        <property name="endpoint" value="${properties.aliyun.oss.endpoint}"/>
        <property name="accessKeyId" value="${properties.aliyun.oss.access.key.id}"/>
        <property name="accessKeySecret" value="${properties.aliyun.oss.access.key.secret}"/>
    </bean>

</beans>